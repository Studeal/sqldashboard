<div class="col-xs-3 sidebar">
    <h2>My database</h2>

    <!-- Database display-->
    <div class="panel-group">
        {% for table in tables %}
                <div class="panel panel-default">
                    <div class="panel-heading tableList">
                        <h4><a data-toggle="collapse" href="#collapse{{loop.index}}">{{ table.Tables_in_sqldashboard }}</a></h4>
                    </div>
                    <div id="collapse{{loop.index}}" class="panel-collapse collapse">
                        <ul class="list-group" >
                            <li class="list-group-item">Field 1</li>
                            <li class="list-group-item">Field 2</li>
                            <li class="list-group-item">Field 3</li>
                        </ul>
                    </div>
                </div>
        {% endfor %}
    </div>
</div>